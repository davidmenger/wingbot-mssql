  "scripts": {
    "devel:sql:start": "(docker rm sqlwebchattest || true) && docker run -e 'ACCEPT_EULA=Y' -e 'SA_PASSWORD=NeotravujPotvoro1' -p 1433:1433 --name sqlwebchattest -d mcr.microsoft.com/mssql/server:2017-latest && sleep 10 && docker exec -it sqlwebchattest /opt/mssql-tools/bin/sqlcmd -S localhost -U SA -P 'NeotravujPotvoro1' -Q 'CREATE DATABASE webchattest'",
    "devel:sql:stop": "docker kill sqlwebchattest && docker rm sqlwebchattest",
    "doc": "gitbook install ./docSrc && gitbook build ./docSrc && rm -rf ./docs && mv ./docSrc/_book ./docs",
    "start": "node ./bin/www.js",
    "test": "npm run test:lint && npm run test:coverage && npm run test:coverage:threshold && npm run test:public:coverage && npm run test:public:coverage:threshold",
    "test:backend": "./node_modules/mocha/bin/mocha --opts ./mocha.opts ./test/lib ./test/genesys ./test/routes",
    "test:coverage": "nyc --reporter=html  mocha ./test/lib ./test/routes && nyc report",
    "test:coverage:threshold": ":",
    "test:coverage:threshold:disabled": "nyc check-coverage --lines 80 --functions 80 --branches 70",
    "test:lint": "npm run test:lint:server && npm run test:lint:public",
    "test:lint:server": "eslint --ext .js bin lib *.js test config routes genesys",
    "test:lint:public": "eslint --ext .js --ext .jsx public test/public",
    "test:public:coverage": "karma start karma.coverage.conf.js",
    "test:public:coverage:threshold": ":",
    "test:public:coverage:threshold:disabled": "istanbul check --branches 70 --functions 80 --statements 80 --lines 80 coverage/**/coverage*.json",
    "test:public:debug": "karma start karma.debug.conf.js --browsers=Chrome --single-run=false",
    "test:public": "karma start",
    "test:public:mac": "karma start --browsers=Chrome,Firefox,Safari,Opera",
    "build": "node -v && node ./bin/build.js",
    "build:production": "node ./bin/build.js -s production",
    "build:staging": "node ./bin/build.js -s staging",
    "build:development": "node ./bin/build.js -s development",
    "deploy:staging": "node ./bin/build.js -s staging && sls deploy -s staging",
    "deploy:production": "node ./bin/build.js -s production && sls deploy -s production",
    "deploy:production:fe": "node ./bin/build.js -s production && sls s3deploy -s production",
    "deploy:staging:fe": "node ./bin/build.js -s staging && sls s3deploy -s staging"
  },
